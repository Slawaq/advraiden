{
  "extends": "eslint-config-airbnb",
  "env": {
    "node": true
  },
  "globals": {
      "__": true
    },
  "rules": {
    "id-length": 0,
    "no-else-return": 1,
    "no-unused-vars": 1,
    "no-param-reassign": 1,
    "no-console": 0,
    // Temporarily disabled due to babel-eslint issues:
    "block-scoped-var": 0,
    "padded-blocks": 0,
    "no-multi-spaces": 0,
    "prefer-const": 0,
    "comma-dangle": 0,
    "one-var": 0,
  },
  "parser": "babel-eslint",
  "plugins": [
  ]
}