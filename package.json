  "description": "TDS",
  "main": "index.js",
  "scripts": {
    "build:client": "node src/client/build",
    "watch:client": "npm run build:client -- watch",
    "watch:admin": "parallelshell \"npm run watch:client\" \"nodemon -i '**/handler/redirect/**/*.*' -i '**/client/**/*.*' -i state admin.js\"",
    "watch:redirect": "nodemon -i '**/handler/admin/**/*.*' -i '**/client/**/*.*' -i state redirect.js",
    "watch:servers": "nodemon -i '**/client/**/*.*' -i state",
    "admin": "npm run build:client -- prod && node --es_staging admin.js",
    "redirect": "node redirect.js",
    "dev": "parallelshell \"npm run watch:client\" \"npm run watch:servers\""
  },
